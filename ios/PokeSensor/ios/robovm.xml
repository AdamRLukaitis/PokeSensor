<config>
	<executableName>${app.executable}</executableName>
	<mainClass>${app.mainclass}</mainClass>
	<os>ios</os>
	<arch>thumbv7</arch>
	<target>ios</target>
	<iosInfoPList>Info.plist.xml</iosInfoPList>
	<resources>
		<resource>
			<directory>../android/assets</directory>
			<includes>
				<include>**</include>
			</includes>
			<skipPngCrush>true</skipPngCrush>
		</resource>
		<resource>
			<directory>resources</directory>
			<includes>
				<include>**</include>
			</includes>
		</resource>
	</resources>
	<forceLinkClasses>
		<pattern>com.android.okhttp.HttpHandler</pattern>
		<pattern>com.android.okhttp.HttpsHandler</pattern>
		<pattern>com.android.org.conscrypt.**</pattern>
		<pattern>com.android.org.bouncycastle.jce.provider.BouncyCastleProvider
		</pattern>
		<pattern>com.android.org.bouncycastle.jcajce.provider.keystore.BC$Mappings
		</pattern>
		<pattern>com.android.org.bouncycastle.jcajce.provider.keystore.bc.BcKeyStoreSpi
		</pattern>
		<pattern>com.android.org.bouncycastle.jcajce.provider.keystore.bc.BcKeyStoreSpi$Std
		</pattern>
		<pattern>com.android.org.bouncycastle.jce.provider.PKIXCertPathValidatorSpi
		</pattern>
		<pattern>com.android.org.bouncycastle.jce.provider.RFC3280CertPathUtilities
		</pattern>
		<pattern>com.android.org.bouncycastle.crypto.digests.AndroidDigestFactoryOpenSSL
		</pattern>
		<pattern>org.apache.harmony.security.provider.**</pattern>
		<pattern>de.tomgrill.gdxdialogs.ios.IOSGDXDialogs</pattern>
		<pattern>com.logickllc.**</pattern>
		<pattern>com.lemmingapex.**</pattern>
		<pattern>POGOProtos.**</pattern>
		<pattern>com.pokegoapi.**</pattern>
		<pattern>net.jpountz.**</pattern>
		<pattern>org.robovm.bindings.amazon.**</pattern>
		<pattern>org.robovm.bindings.aerserv.**</pattern>
		<pattern>org.robovm.bindings.appirater.**</pattern>
		<pattern>org.robovm.bindings.firebase.**</pattern>
		<pattern>java.util.logging.ConsoleHandler</pattern>
		<pattern>com.squareup.moshi.**</pattern>
		<pattern>com.badlogic.gdx.pay.**</pattern>
		<pattern>POGOProtos.Settings.Master.PokemonSettingsOuterClass</pattern>
	</forceLinkClasses>
	<libs>
		<lib>z</lib>
		<lib>xml2</lib>
		<lib>AmazonAd.a</lib>
		<lib>AerServSDK.a</lib>
		<lib>AERMoatMobileAppKit.a</lib>
		<lib>GoogleMobileAds.a</lib>
		<lib>libappirater.a</lib>
	</libs>

	<frameworkPaths>
		<path>frameworks</path>
	</frameworkPaths>

	<frameworks>
		<framework>UIKit</framework>
		<framework>MapKit</framework>
		<framework>OpenGLES</framework>
		<framework>QuartzCore</framework>
		<framework>CoreGraphics</framework>
		<framework>OpenAL</framework>
		<framework>AudioToolbox</framework>
		<framework>AVFoundation</framework>
		<framework>MediaPlayer</framework>
		<framework>EventKit</framework>
		<framework>EventKitUI</framework>
		<framework>CoreTelephony</framework>
		<framework>SystemConfiguration</framework>
		<framework>CoreLocation</framework>
		<framework>CoreMedia</framework>
		<framework>MessageUI</framework>
		<framework>SafariServices</framework>
		<framework>GLKit</framework>
		<framework>JavaScriptCore</framework>
		<framework>CoreMotion</framework>
		<framework>CoreVideo</framework>
		<framework>Security</framework>
		<framework>CFNetwork</framework>
		<framework>Foundation</framework>
		<framework>FirebaseAnalytics</framework>
		<framework>FirebaseAuth</framework>
		<framework>FirebaseCore</framework>
		<framework>FirebaseInstanceID</framework>
		<framework>GoogleInterchangeUtilities</framework>
		<framework>GoogleSymbolUtilities</framework>
		<framework>GoogleToolboxForMac</framework>
		<framework>GTMSessionFetcher</framework>
	</frameworks>
	<weakFrameworks>
		<framework>AdSupport</framework>
		<framwwork>StoreKit</framwwork>
	</weakFrameworks>
</config>
